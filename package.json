{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "ingestion-api",
  "version": "1.0.0",
  "description": "Priority-based data ingestion API with rate limiting",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles"
  },
  "dependencies": {
    "express": "^4.18.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000
  },
  "keywords": [
    "api",
    "ingestion",
    "priority-queue",
    "rate-limiting",
    "nodejs",
    "express"
  ],
  "author": "Chirag",
  "license": "MIT"
}
